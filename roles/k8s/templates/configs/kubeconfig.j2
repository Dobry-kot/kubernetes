apiVersion: v1
clusters:
- cluster:
    certificate-authority: {{ pki_path_ca }}/root-ca.pem
    server: https://127.0.0.1:6443
  name: default-cluster
contexts:
- context:
    cluster: default-cluster
    namespace: default
    user: default-auth
  name: default-context
current-context: default-context
kind: Config
preferences: {}
users:
- name: default-auth
  user:
    client-certificate: {{ pki_path_certs }}/{{ k8s_component }}/{{ kubeconfig_cert }}.pem
    client-key: {{ pki_path_certs }}/{{ k8s_component }}/{{ kubeconfig_cert }}-key.pem
