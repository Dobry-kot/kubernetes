apiVersion: v1
kind: ConfigMap
metadata:
    name: cluster-info
    namespace: kube-public
data:
    kubeconfig: |
        apiVersion: v1
        clusters:
        - cluster:
            certificate-authority-data: "{{ ca_content.content }}"
            server: https://{{ external_api_ip }}:6443
        name: ""
        contexts: null
        current-context: ""
        kind: Config
        preferences: {}
        users: null